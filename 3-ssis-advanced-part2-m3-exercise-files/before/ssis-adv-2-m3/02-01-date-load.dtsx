      DTS:Expression="&quot;&#xA;;&#xA;WITH DateCTE AS&#xA;(&#xA;&#xA;SELECT CAST(&quot;  + @StartDate + &quot; AS DATE) AS DateValue&#xA;UNION ALL&#xA;SELECT DATEADD(d,1,DateValue)&#xA;FROM DateCTE&#xA;WHERE DATEADD(d,1,DateValue) &lt; &quot; + @EndDate + &quot;&#xA;&#xA;)&#xA;INSERT INTO dw.DimDate&#xA;SELECT&#xA;CAST(CONVERT(CHAR(8),CAST(DateValue AS DATETIME),112) AS INT) AS DateKey&#xA;, CONVERT(CHAR(10), cast(DateValue AS DATETIME), 101) AS CalendarDate&#xA;, DATEPART(dw, DateValue) AS DayNumberOfWeek&#xA;, DATENAME(dw, DateValue) AS EnglishDayNameOfWeek&#xA;, DAY(DateValue) as DayNumberOfMonth&#xA;, DATEPART(dy, DateValue) as DayNumberOfYear&#xA;, DATEPART(wk,DateValue) AS WeekNumberOfYear&#xA;, DATENAME(MONTH,DateValue) AS EnglishMonthName&#xA;, DATEPART(m,DateValue) AS MonthNumberOfYear&#xA;, DATEPART(Quarter ,DateValue) as CalendarQuarter&#xA;, YEAR(DateValue) AS CalendarYear&#xA;&#xA;, CASE&#xA;WHEN DATEPART(Quarter ,DateValue) &lt; 3 THEN&#xA;1&#xA;ELSE&#xA;2&#xA;END AS CalendarHalfNumber&#xA;, &quot; + (DT_STR, 10, 1252) @[User::AuditKey] + &quot;&#xA;&#xA;FROM DateCTE a&#xA;&#xA;OPTION (MAXRECURSION 5000)&#xA;&#xA;--======================================&#xA;-- Populate default low and high values&#xA;--======================================&#xA;;WITH DateCTE AS&#xA;(&#xA;SELECT CAST('01-01-1900' AS DATE) AS DateValue&#xA;UNION ALL&#xA;SELECT CAST('12-31-2999' AS DATE)&#xA;)&#xA;INSERT INTO dw.DimDate&#xA;SELECT&#xA;CAST(CONVERT(CHAR(8),CAST(DateValue AS DATETIME),112) AS INT) AS DateKey&#xA;, CONVERT(CHAR(10), cast(DateValue AS DATETIME), 101) AS CalendarDate&#xA;, DATEPART(dw, DateValue) AS DayNumberOfWeek&#xA;, DATENAME(dw, DateValue) AS EnglishDayNameOfWeek&#xA;, DAY(DateValue) as DayNumberOfMonth&#xA;, DATEPART(dy, DateValue) as DayNumberOfYear&#xA;, DATEPART(wk,DateValue) AS WeekNumberOfYear&#xA;, DATENAME(MONTH,DateValue) AS EnglishMonthName&#xA;, DATEPART(m,DateValue) AS MonthNumberOfYear&#xA;, DATEPART(Quarter ,DateValue) as CalendarQuarter&#xA;, YEAR(DateValue) AS CalendarYear&#xA;&#xA;, CASE&#xA;WHEN DATEPART(Quarter ,DateValue) &lt; 3 THEN&#xA;1&#xA;ELSE&#xA;2&#xA;END AS CalendarHalfNumber&#xA;, &quot; +  (DT_STR, 10, 1252) @[User::AuditKey] + &quot;&#xA;FROM DateCTE a&quot;"
      DTS:IncludeInDebugDump="2345"
