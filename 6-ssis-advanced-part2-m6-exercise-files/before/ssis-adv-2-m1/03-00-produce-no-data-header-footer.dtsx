                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--/****** Script for SelectTopNRows command from SSMS  ******/
SELECT 
      s.[Name] as StoreName
	, [AddressLine1]
	, [AddressLine2]
	, [City]
	, sp.Name as StateProvince
	, [PostalCode]
	, coalesce(sum(soh.TotalDue), 0) as SalesAmount
  FROM [Sales].[Store] s
  join [Person].[BusinessEntityAddress] bea on s.BusinessEntityID = bea.BusinessEntityID
  join [Person].[Address] a on bea.AddressID = a.AddressID
  join [Person].[StateProvince] sp on a.StateProvinceID = sp.StateProvinceID
  join [Sales].[Customer] c on c.StoreID = s.BusinessEntityID
  left outer join [Sales].[SalesOrderHeader] soh on soh.CustomerID = c.CustomerID
  where bea.AddressTypeID = 3
  group by 
        s.[Name] 
	, [AddressLine1]
	, [AddressLine2]
	, [City]
	, sp.Name 
	, [PostalCode]</property>
