                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
	pc.EnglishProductCategoryName,
	convert(int,Left(f.OrderDateKey, 4)) as OrderYear,
	sum(f.SalesAmount) as SalesAmount
FROM
	FactResellerSales f
	JOIN DimProduct p
		ON p.ProductKey = f.ProductKey
	JOIN DimProductSubcategory ps
		ON ps.ProductSubcategoryKey = p.ProductSubcategoryKey
	JOIN DimProductCategory pc
		ON pc.ProductCategoryKey = ps.ProductCategoryKey 
GROUP BY
	pc.EnglishProductCategoryName,
	convert(int,Left(f.OrderDateKey, 4))</property>
