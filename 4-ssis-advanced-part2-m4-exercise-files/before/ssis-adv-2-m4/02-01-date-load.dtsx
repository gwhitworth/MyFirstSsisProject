      <DTS:VariableValue
        DTS:DataType="8">
;
WITH DateCTE AS
(

SELECT CAST( '20050101' AS DATE) AS DateValue
UNION ALL
SELECT DATEADD(d,1,DateValue)
FROM DateCTE
WHERE DATEADD(d,1,DateValue) &lt; '20151231'

)
INSERT INTO dw.DimDate
SELECT
CAST(CONVERT(CHAR(8),CAST(DateValue AS DATETIME),112) AS INT) AS DateKey
, CONVERT(CHAR(10), cast(DateValue AS DATETIME), 101) AS CalendarDate
, DATEPART(dw, DateValue) AS DayNumberOfWeek
, DATENAME(dw, DateValue) AS EnglishDayNameOfWeek
, DAY(DateValue) as DayNumberOfMonth
, DATEPART(dy, DateValue) as DayNumberOfYear
, DATEPART(wk,DateValue) AS WeekNumberOfYear
, DATENAME(MONTH,DateValue) AS EnglishMonthName
, DATEPART(m,DateValue) AS MonthNumberOfYear
, DATEPART(Quarter ,DateValue) as CalendarQuarter
, YEAR(DateValue) AS CalendarYear

, CASE
WHEN DATEPART(Quarter ,DateValue) &lt; 3 THEN
1
ELSE
2
END AS CalendarHalfNumber
, 0

FROM DateCTE a

OPTION (MAXRECURSION 5000)

--======================================
-- Populate default low and high values
--======================================
;WITH DateCTE AS
(
SELECT CAST('01-01-1900' AS DATE) AS DateValue
UNION ALL
SELECT CAST('12-31-2999' AS DATE)
)
INSERT INTO dw.DimDate
SELECT
CAST(CONVERT(CHAR(8),CAST(DateValue AS DATETIME),112) AS INT) AS DateKey
, CONVERT(CHAR(10), cast(DateValue AS DATETIME), 101) AS CalendarDate
, DATEPART(dw, DateValue) AS DayNumberOfWeek
, DATENAME(dw, DateValue) AS EnglishDayNameOfWeek
, DAY(DateValue) as DayNumberOfMonth
, DATEPART(dy, DateValue) as DayNumberOfYear
, DATEPART(wk,DateValue) AS WeekNumberOfYear
, DATENAME(MONTH,DateValue) AS EnglishMonthName
, DATEPART(m,DateValue) AS MonthNumberOfYear
, DATEPART(Quarter ,DateValue) as CalendarQuarter
, YEAR(DateValue) AS CalendarYear

, CASE
WHEN DATEPART(Quarter ,DateValue) &lt; 3 THEN
1
ELSE
2
END AS CalendarHalfNumber
, 0
FROM DateCTE a</DTS:VariableValue>
